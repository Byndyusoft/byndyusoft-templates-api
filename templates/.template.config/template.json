   "author":"Byndyusoft",
   "classifications":[
      "Web"
   ],
   "projectUrl":"https://github.com/Byndyusoft/Byndyusoft.TemplateApi",
   "description":"",
   "name":"Byndyusoft api template",
   "defaultName":"Byndyusoft.Template.Api",
   "identity":"Byndyusoft.Template.Api",
   "groupIdentity":"bs",
   "tags":{
      "language":"C#",
      "type":"project"
   },
   "shortName":"bsapi",
